<div class="container">
<h1>Chime Homepage</h1>
<h2>Welcome {{org}}</h2>
<h3>Send an SMS</h3>
<div>
  <form>
    <label for="message">Compose Text Message</label>
    <input type="text" id="message">
    <button id="send">Send</button>
  </form>
</div>
<div>
<h3>Sent messages</h3>
{{#each messages}}
  <div class="sms-div">
    <p class="sms-date">{{datetime}}</p>
    <p class="sms-text">{{message}}</p>
    <!--<small>{{date}}</small>-->
  </div>
  {{/each}}
</div>
</div>

